{
  "name": "scitran-organizer",
  "productName": "SciTran Organizer",
  "version": "0.0.1",
  "description": "SciTran Data Organizer",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "start-hot": "cross-env HOT=1 electron main.js",
    "dev": "concurrently --kill-others \"npm run watch\" \"npm run start-hot\"",
    "lint": "eslint app *.js",
    "clean": "rm -rf ./build ./css ./dist",
    "clean:osx": "rm -rf ./dist/osx",
    "clean:win": "rm -rf ./dist/win",
    "pack": "npm run pack:osx && npm run pack:win",
    "pack:osx": "npm run clean && npm run webpack && electron-packager . --out=dist/osx --platform=darwin --arch=x64 --icon=assets/osx/logo.icns --prune --ignore=assets --ignore=dist --ignore=scss",
    "pack:win": "npm run clean && npm run webpack && electron-packager . --out=dist/win --platform=win32 --arch=ia32 --icon=assets/win/logo.ico --prune--ignore=assets --ignore=dist --ignore=scss",
    "webpack": "webpack && gulp sass",
    "watch": "webpack-dev-server --hot --inline",
    "node": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scitran/organizer.git"
  },
  "keywords": [
    "SciTran",
    "Data",
    "Organizer",
    "DICOM",
    "NIfTI",
    "BIDS",
    "OpenfMRI"
  ],
  "author": "SciTran Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scitran/organizer/issues"
  },
  "homepage": "https://github.com/scitran/organizer#readme",
  "devDependencies": {
    "concurrently": "^2.2.0",
    "cross-env": "^1.0.8",
    "electron-packager": "^7.0.7",
    "electron-prebuilt": "^1.3.4",
    "eslint": "^2.13.1",
    "eslint-loader": "^1.5.0",
    "gulp": "^3.9.1",
    "gulp-sass": "^2.3.2",
    "json-loader": "^0.5.4",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0"
  },
  "dependencies": {
    "angular": "^1.5.8",
    "angular-ui-router": "^0.3.1",
    "archiver": "^1.0.1",
    "dicom-parser": "^1.7.4",
    "request": "^2.74.0",
    "rx": "^4.1.0"
  }
}
